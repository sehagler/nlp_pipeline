version: 6.2
creationDate: '2019-01-29 17:44:49'
query:
  document:
  - region:
      view: {height: 254, width: 793}
      snid: hc.titled_section
      pt: Titled Section
      where:
      - region:
          id: region2
          snid: hc.section_title
          pt: Section Title
          where:
          - alternative:
            - phrase:
                displayChildren: false
                of: [MATERIALS, RECEIVED]
            - SUMMARY
      - region:
          columnOptions: [Text]
          showWholeRegion: true
          view: {height: 124, width: 530}
          snid: hc.section_body
          pt: Section Body
          where:
          - phrase: [bone, marrow]
          - phrase:
              quantifier: {minimum: 0, maximum: 0}
              of: [peripheral, blood]
          - link: {path: ../../General/cancer/blast_%/bone_marrow_blast_%/bone_marrow_blast_%_query.i2qy, id: link3}
  - link: {path: ../tools_lib/document_filter_query.i2qy}
output:
  columns:
  - {id: region2}
  - id: value1
    columnOptions: [Empty]
    isValueColumn: true
  - id: link3.phrase1
    itemOptions: {}
    linkItemId: link3
  useOutputEditor: true
