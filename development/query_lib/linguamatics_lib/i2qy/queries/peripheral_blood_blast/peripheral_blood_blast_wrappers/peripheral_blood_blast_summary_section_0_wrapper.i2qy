  - region:
      snid: hc.titled_section
      pt: Titled Section
      where:
      - region:
          id: region1
          snid: hc.section_title
          pt: Section Title
          where:
          - alternative: [DX]
      - region:
          snid: hc.section_body
          pt: Section Body
          where:
          - phrase:
            - word:
                text: peripheral
                quantifier: {minimum: 0, maximum: 1}
            - blood
          - phrase:
              quantifier: {minimum: 0, maximum: 0}
              of: [bone, marrow]
          - link: {path: ../peripheral_blood_blast_query_snippet.i2qy, id: link1}
