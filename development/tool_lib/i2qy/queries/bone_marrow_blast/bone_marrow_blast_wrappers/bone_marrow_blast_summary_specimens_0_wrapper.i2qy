version: 6.3
creationDate: '2019-01-29 17:32:46'
query:
  document:
  - region:
      snid: hc.titled_section
      pt: Titled Section
      where:
      - region:
          id: region1
          snid: hc.section_title
          pt: Section Title
          where:
          - alternative: [DIAGNOSIS]
      - region:
          snid: hc.section_body
          pt: Section Body
          where:
          - region:
              columnOptions: [Text]
              showWholeRegion: true
              snid: hc.specimen
              pt: Specimen Description
              where:
              - shadowRegion: {id: shadow region1, snid: hc.specimen_id, pt: Specimen Id}
              - phrase: [bone, marrow]
              - phrase:
                  quantifier: {minimum: 0, maximum: 0}
                  of: [peripheral, blood]
              - link: {path: ../../../cancer/blast_%/bone_marrow_blast_%/bone_marrow_blast_%_query.i2qy, id: link3}
output:
  columns:
  - {id: region1}
  - {id: shadow region1}
  - id: link3.phrase1
    itemOptions: {}
    linkItemId: link3
  useOutputEditor: true
creator: haglers
