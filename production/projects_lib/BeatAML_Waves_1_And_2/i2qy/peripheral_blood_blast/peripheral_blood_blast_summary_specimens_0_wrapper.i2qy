version: 6.2
creationDate: '2019-01-29 18:22:39'
query:
  document:
  - region:
      snid: hc.titled_section
      pt: Titled Section
      where:
      - region:
          id: region1
          snid: hc.section_title
          pt: Section Title
          where:
          - alternative:
            - phrase:
                displayChildren: false
                of: [MATERIALS, RECEIVED]
            - SUMMARY
      - region:
          snid: hc.section_body
          pt: Section Body
          where:
          - region:
              columnOptions: [Text]
              showWholeRegion: true
              snid: hc.specimen
              pt: Specimen Description
              where:
              - shadowRegion: {id: shadow region1, snid: hc.specimen_id, pt: Specimen Id}
              - phrase:
                - word:
                    text: peripheral
                    quantifier: {minimum: 0, maximum: 1}
                - blood
              - phrase:
                  quantifier: {minimum: 0, maximum: 0}
                  of: [bone, marrow]
              - link: {path: ../../General/cancer/blast_%/peripheral_blood_blast_%/peripheral_blood_blast.i2qy, id: link2}
  - link: {path: ../tools_lib/document_filter_query.i2qy}
output:
  columns:
  - {id: region1}
  - {id: shadow region1}
  - id: link2.phrase1
    itemOptions: {}
    linkItemId: link2
  useOutputEditor: true
creator: haglers
